<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CarList</title>
</head>
<body>
    <% if(typeof cheading != 'undefined'){ %>
    <section>
        <h1><%= cheading %></h1>
    </section>
    <% } %>
    <section>
        <ul>
            <% carlist.forEach(function(cars){ %>
            <h1><%= cars.heading %></h1>
            <li><a href="/car/<%= cars.id %>"><%= cars.name %></a></li>
            <% }); %>
        </ul>
    </section>
</body>
</html>